<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
         <record model="ir.ui.view" id="product_category_form_view_inherit">
            <field name="name">product.category.form.view.inherit</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view" />
            <field name="arch" type="xml" >
                <field name="type" position="after">
                    <field name="tariff_rate"/>
                </field>
            </field>
        </record>
        
         <record model="ir.ui.view" id="product_normal_form_view_inherit">
            <field name="name">product.normal.form.view.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view" />
            <field name="arch" type="xml" >
                <field name="volume" position="after">
                    <field name="tariff_rate"/>
                </field>
            </field>
        </record>
        
    </data>
</openerp>
